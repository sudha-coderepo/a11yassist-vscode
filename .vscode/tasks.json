{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "npm: compile",
      "type": "shell",
      "command": "npm",
      "args": [
        "run",
        "compile"
      ],
      "isBackground": false,
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": {
        "owner": "typescript",
        "fileLocation": "relative",
        "pattern": {
          "regexp": "^([^(]*\\([0-9]+,[0-9]+\\):\\s+error\\s+TS\\d+:.*$)",
          "file": 1
        }
      }
    }
  ]
}
